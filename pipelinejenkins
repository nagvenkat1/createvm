pipeline {
    agent {label 'JDK11'}
        stages {
        stage('vcs') {
            steps {
                git branch: 'main', url: 'https://github.com/nagvenkat1/createvm.git'
            }
        }
         stage('create') {
            steps {
                sh'curl -sL https://aka.ms/InstallAzureCLIDeb | sudo bash'
                sh'chmod 777 jenkinsfile.sh '
                sh './jenkinsfile.sh'
            }
         }
     }

}






        
